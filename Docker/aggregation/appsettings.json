{
  "ConnectionStrings": {
    //"DefaultConnectionString": "Host=192.168.88.138;Port=5432;Database=Aggregation;Username=postgres;Password=12345678" // Gvantsa
    "DefaultConnectionString": "Host=192.168.88.142;Port=5432;Database=Aggregation;Username=postgres;Password=Test1234!", // Akaki
    "MongoDBConnectionString": "mongodb://localhost:27017", // Akaki
    "RedisConnectionString": "192.168.88.138:6379,abortConnect=false" // Akaki
  },
  "RabbitMQSettings": {
    "ConnectionName": "aggregation-service",
    "Retries": 3,
    "RetryInterval": 2,
    "ConventionsCasing": "snakeCase",
    "Host": "192.168.88.142",
    "Port": "5672",
    "User": "guest",
    "Password": "guest",
    "RequestedConnectionTimeout": "00:00:30",
    "RequestedHeartbeat": "00:01:00",
    "SocketReadTimeout": "00:00:30",
    "SocketWriteTimeout": "00:00:30",
    "ContinuationTimeout": "00:00:20",
    "HandshakeContinuationTimeout": "00:00:10",
    "NetworkRecoveryInterval": "00:00:05",
    "Exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "deliveries"
    },
    "Queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
