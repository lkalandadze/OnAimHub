{
  "Routes": [
    {
      "DownstreamPathTemplate": "/wheelapi/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "wheelapi",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/wheelapi/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ]
    },
    {
      "DownstreamPathTemplate": "/leaderboardapi/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "leaderboardapi",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/leaderboardapi/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ]
    },
    {
      "DownstreamPathTemplate": "/penaltykicksapi/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "penaltykicksapi",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/penaltykicksapi/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000",
    "ServiceDiscoveryProvider": {
      "Type": "Consul",
      "Scheme": "http",
      "Host": "consul",
      "Port": 8500
    }
  }
}